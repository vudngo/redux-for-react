{
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true
    },
    "parserOptions": {
        "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
    },
    "rules": {
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
    }
}
